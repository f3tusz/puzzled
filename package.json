  "devDependencies": {
    "body-scroll-lock": "^3.0.2",
    "bootstrap": "^4.5.0",
    "browser-sync": "^2.24.7",
    "browser-sync-webpack-plugin": "^2.0.1",
    "glob-all": "^3.1.0",
    "headroom.js": "^0.11.0",
    "i": "^0.3.6",
    "jquery": "^3.3.1",
    "laravel-mix": "^5.0.1",
    "lazysizes": "^5.1.0",
